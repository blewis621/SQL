SELECT * FROM category


INSERT INTO category (name)
VALUES
('Thriller'),
('Crime'),
('Mystery'),
('Romance'),
('War')


SELECT film_id, title
FROM film
WHERE title='African Egg'


SELECT category_id, film_id
FROM film_category
WHERE film_id=5


UPDATE film_category
SET category_id=17
WHERE film_id=5


SELECT *
FROM film_category
WHERE film_id=5


DELETE FROM category
WHERE name = 'mystery'
